@if (StarCount == 0)
{                               {
        <MudIcon Icon="@Icons.Material.Rounded.StarBorder" Color="MudBlazor.Color.Default" Class="m-0 p-0" />
        <MudIcon Icon="@Icons.Material.Rounded.StarBorder" Color="MudBlazor.Color.Default" />
}                     }

@if (StarCount == 1)
{
    <MudIcon Icon="@Icons.Material.Rounded.Star" Title="Part One" Color="MudBlazor.Color.Warning" />
    <MudIcon Icon="@Icons.Material.Rounded.StarBorder" Title="Part Two" Color="MudBlazor.Color.Default" />
}

@if (StarCount == 2)
{
    <MudIcon Icon="@Icons.Material.Rounded.Star" Title="Part One" Color="MudBlazor.Color.Warning" />
    <MudIcon Icon="@Icons.Material.Rounded.Star" Title="Part Two" Color="MudBlazor.Color.Warning" />
}
@code {
    [Parameter]
    public int StarCount { get; set; }
}
